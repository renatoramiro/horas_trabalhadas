<%= form_for([@project_membership.project, @project_membership]) do |f| %>
  <%= f.hidden_field :project_id %>
  <%= f.hidden_field :user_id %>
  <%= f.hidden_field :joined %>

  <p>Projeto: <%= @project_membership.project.name %> </p>
  <p>Data de Início: <%= @project_membership.joined %> </p>
  
  <p>
    <%= f.submit "Confirmar Associação a projeto" %>
  </p>
<% end %>